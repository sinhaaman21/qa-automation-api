    "cypress": "^9.5.0",
    "cypress-postgres": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.14.0"
  },
  "dependencies": {
    "allure-commandline": "^2.17.2",
    "cypress-postgresql": "^1.0.8",
    "pg": "^8.7.1"
  }
}